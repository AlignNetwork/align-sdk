{
  "name": "@align-network/align-sdk",
  "version": "1.1.0",
  "author": "0xglu",
  "main": "./dist/index.js",
  "types": "./dist/index.d.ts",
  "typings": "./dist/index.d.ts",
  "sideEffects": false,
  "type": "module",
  "repository": {
    "type": "git",
    "url": "git+https://github.com/AlignNetwork/align-sdk.git"
  },
  "tsup": {
    "entry": [
      "./src/**/*.ts"
    ],
    "clean": true,
    "format": [
      "esm"
    ],
    "sourcemap": true,
    "sideEffects": false,
    "dts": true
  },
  "exports": {
    ".": {
      "types": "./dist/index.d.ts",
      "default": "./dist/index.js"
    },
    "./read": {
      "types": "./dist/read/index.d.ts",
      "default": "./dist/read/index.js"
    }
  },
  "scripts": {
    "prepare": "husky",
    "build": "rimraf dist && tsup && tsc --emitDeclarationOnly --declaration",
    "dev": "pnpm run build --watch"
  },
  "devDependencies": {
    "@commitlint/cli": "^19.2.2",
    "@commitlint/config-conventional": "^19.2.2",
    "@semantic-release/changelog": "^6.0.3",
    "@semantic-release/git": "^10.0.1",
    "@types/bun": "latest",
    "husky": "^9.0.11",
    "semantic-release": "^23.0.8",
    "viem": "^2.9.23"
  },
  "peerDependencies": {
    "typescript": "^5.0.0"
  },
  "peerDependenciesMeta": {
    "typescript": {
      "optional": true
    }
  },
  "description": "Align Attestation Station SDK",
  "homepage": "https://align.network",
  "license": "MIT",
  "private": false,
  "publishConfig": {
    "access": "public",
    "registry": "https://registry.npmjs.org/"
  },
  "dependencies": {
    "@tanstack/react-query": "^5.31.0",
    "rimraf": "^5.0.5",
    "tsup": "^8.0.2"
  }
}
